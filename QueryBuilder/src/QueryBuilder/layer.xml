<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="QueryBuilder-QueryBuilderAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="QueryBuilder.Bundle"/>
                <attr name="component" methodvalue="QueryBuilder.QueryBuilderTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="QueryBuilder.Bundle#CTL_QueryBuilderAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="QueryBuilderAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/QueryBuilder-QueryBuilderAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="QueryBuilderTopComponent.settings" url="QueryBuilderTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="QueryBuilderTopComponent.wstcref" url="QueryBuilderTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
